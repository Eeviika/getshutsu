; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[USAPI]

Caching/DoCaching=true
Caching/CacheObjects=true
Caching/CacheResources=true
Caching/CacheRooms=true
Caching/CacheAudio=false
Caching/CacheLimit=0
Caching/CacheObjectsOnStart=true
Caching/CacheResourcesOnStart=true
Caching/CacheRoomsOnStart=true
Caching/CacheAudioOnStart=false
Paths/ObjectsPath="res://objects"
Paths/ResourcesPath="res://resources"
Paths/AudioPath="res://audio"
Paths/ScriptsPath="res://scripts"
Paths/AssetsPath="res://assets"
Paths/RoomsPath="res://rooms"
Console/ConsoleEnabled=true
Console/AllowCustomConsoleCommands=false
Misc/UseDefaultSettingsMenu=true

[application]

config/name="Getshutsu"
config/version="predemo-0.0.1"
config/tags=PackedStringArray("prioritize", "work_on_now")
config/use_custom_user_dir=true
config/custom_user_dir_name="Uncoded Studios/Getshutsu"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://placeholder.svg"

[autoload]

GlobalEnums="*res://scripts/enums.gd"
usapi="*res://addons/uncoded_studios_api/plugin.tscn"

[debug]

file_logging/log_path="user://logs/game.log"

[display]

window/stretch/mode="viewport"

[editor]

naming/node_name_casing=1
naming/scene_name_casing=0

[editor_plugins]

enabled=PackedStringArray("res://addons/ResourcePlus/plugin.cfg", "res://addons/uncoded_studios_api/plugin.cfg")

[filesystem]

import/blender/enabled=false
import/fbx/enabled=false

